{
    "name": "neo4j-gqllib-starter-server",
    "version": "1.0.0",
    "private": true,
    "description": "",
    "main": "./dist/src/index.js",
    "scripts": {
        "start": "DEBUG='@neo4j/graphql:execute,Server:*,Neo4j:*' nodemon --watch '.--inspect /src/**/*.ts' --exec ../../../node_modules/.bin/ts-node -r tsconfig-paths/register --project ./src/tsconfig.json ./src/index.ts",
        "build": "tsc  --project src/",
        "seed": "ts-node -r tsconfig-paths/register --project ./src/tsconfig.json ./src/seeder.ts",
        "test": "jest",
        "docker:build": "docker build . -t koakh/neo4j-gqllib-starter-server:latest",
        "docker:run": "docker run -p 8080:8080 -d koakh/neo4j-gqllib-starter-server:latest",
        "docker:run:debug": "docker run -it koakh/neo4j-gqllib-starter-server:latest /bin/sh"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@neo4j/graphql": "^3.13.0",
        "@neo4j/graphql-ogm": "^3.13.0",
        "@neo4j/graphql-plugin-auth": "^1.1.0",
        "apollo-server-express": "^3.11.1",
        "bcrypt": "^5.1.0",
        "debug": "^4.3.4",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "express-rate-limit": "^6.7.0",
        "graphql": "^16.6.0",
        "jsonwebtoken": "^8.5.1",
        "neo4j-driver": "^5.3.0"
    },
    "devDependencies": {
        "@faker-js/faker": "^7.6.0",
        "@types/bcrypt": "^5.0.0",
        "@types/debug": "^4.1.7",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.14",
        "@types/jest": "^29.2.3",
        "@types/jsonwebtoken": "^8.5.9",
        "@types/node": "^18.11.10",
        "cross-env": "^7.0.3",
        "jest": "^29.3.1",
        "nodemon": "^2.0.20",
        "randomstring": "^1.2.3",
        "ts-jest": "^29.0.3",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.1.1",
        "typescript": "^4.9.3"
    }
}
